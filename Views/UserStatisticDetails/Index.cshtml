@model IEnumerable<CarRent.Models.UserStatisticDetails>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfRent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalRentTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YearOfRegister)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfRent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalRentTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YearOfRegister)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.StaticticsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.StaticticsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.StaticticsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
